{
  "env": {
    "browser": false,
    "node": true,
    "es6": true
  },
  "plugins": [
    "testcafe"
  ],
  "extends": "plugin:testcafe/recommended",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true,
      "jsx": false
    }
  }
}
